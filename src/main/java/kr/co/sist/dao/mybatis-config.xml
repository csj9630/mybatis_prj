<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="properties/database.properties"/>
<typeAliases>
<typeAlias type="day1224.EmpDTO" alias="eDTO"/>
<typeAlias type="day1226.EmpDTO" alias="empDTO"/>
<typeAlias type="day1226.EmpDomain" alias="empDomain"/>
<typeAlias type="day1226.ZipcodeDomain" alias="zipDomain"/><!-- 우편번호검색결과 저장용 도메인 -->
<typeAlias type="day1226.EmpAllDomain" alias="eaDomain"/><!-- 우편번호검색결과 저장용 도메인 -->
<typeAlias type="day1226.CarModelDomain" alias="cmoDomain"/><!-- 제조사, 모델, 년식, 가격,옵션, 배기량, 이미지 ,입력일 저장용 domain-->
<typeAlias type="kr.co.sist.car.CarModelDomain" alias="carDomain"/><!-- 위 것을 동적 셀렉트용으로 사용-->
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${id}"/>
        <property name="password" value="${pass}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="day1219/dept.xml"/>
    <mapper resource="day1223/examMapper.xml"/><!--insert 연습  -->
    <mapper resource="day1224/examMapper2.xml"/><!--update, delete 연습  -->
    <mapper resource="day1224/examMapper3.xml"/><!--select 및 각종 sql문 연습  -->
    <mapper resource="day1226/examMapper4.xml"/><!--select 및 각종 sql문 연습  -->
    <mapper resource="kr/co/sist/car/carMapper.xml"/><!--자동차 모델 select박스 동적으로 넣기  -->
    <mapper resource="kr/co/sist/board/boardMapper.xml"/>
  </mappers>
</configuration>